function withList(action = "+", what = {1,2,3,4}) {
  if (action == "+") {
    result = 0;
    for (arg : what) {
      result += arg;
    }
    return result;
  } elif (action == "*") {
    result = 1;
    for (arg : what) {
      result *= arg;
    }
    return result;
  }
  return "Unknown action: " + action;
}
Test(withList(), 10);
Test(withList("*"), 24);
Test(withList("+", {5,10,15}), 30);

